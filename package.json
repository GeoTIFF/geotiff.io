    "debug-docs": "npm run link-repos && npm run compile-linked && npm run build && cd docs && http-server",
    "link-repos": "npm link ~/geoblaze && cd ~/geoblaze && npm link ~/georaster && npm link ~/georaster-layer-for-leaflet",
