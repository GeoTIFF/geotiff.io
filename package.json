{
  "name": "geotiff.io",
  "version": "0.1.1",
  "private": true,
  "dependencies": {
    "chroma-js": "^1.3.7",
    "file-saver": "^1.3.8",
    "fuse.js": "^3.2.1",
    "geoblaze": "^0.1.7",
    "georaster-layer-for-leaflet": "0.1.3",
    "leaflet": "^1.3.4",
    "leaflet-draw": "^1.0.2",
    "leaflet-geosearch": "^2.5.0",
    "less": "^2.7.3",
    "offline-plugin": "^5.0.5",
    "pako": "^1.0.6",
    "prop-types": "^15.6.2",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-modal": "^3.1.11",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "recompose": "^0.26.0",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "sweetalert-react": "^0.4.11",
    "underscore": "^1.8.3",
    "webpack-bundle-analyzer": "^2.13.1"
  },
  "devDependencies": {
    "babel-loader": "^7.1.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "cname-webpack-plugin": "^1.0.3",
    "copy-webpack-plugin": "^4.5.2",
    "css-loader": "^0.28.11",
    "eslint": "^5.12.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "favicons-webpack-plugin": "0.0.9",
    "html-webpack-plugin": "^3.2.0",
    "less-loader": "^4.1.0",
    "style-loader": "^0.21.0",
    "tapable": "^1.0.0",
    "webpack": "^4.13.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4",
    "webpack-merge": "^4.1.3"
  },
  "scripts": {
    "analyze": "webpack --config webpack.analyze.js",
    "build": "node ./node_modules/webpack/bin/webpack.js --config webpack.prod.js",
    "compile-linked": "cd ~/geotiff.js && npm run build && cd ~/georaster && npm run babel && cd ~/georaster-layer-for-leaflet && npm run babel && cd ~/geoblaze && npm run babel && npm run build && cd ~/geotiff.io && npm run less",
    "debug-chain": "npm run compile-linked && npm run start",
    "debug-docs": "npm run link-repos && npm run compile-linked && npm run build && cd docs && http-server",
    "install-low-memory": "rm -fr node_modules* && curl -O https://s3.amazonaws.com/geotiff.io/node_modules.zip && unzip -q node_modules.zip && rm node_modules.zip",
    "link-repos": "npm link ~/geoblaze && cd ~/geoblaze && npm link ~/georaster && npm link ~/georaster-layer-for-leaflet",
    "lint": "eslint src",
    "start": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --config webpack.dev.js",
    "start-on-server": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --config webpack.dev.js --host 0.0.0.0 --port 80"
  }
}
