  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "compile-linked": "cd ~/geotiff.js && npm run build && cd ~/georaster && npm run babel && cd ~/georaster-layer-for-leaflet && npm run babel && cd ~/geoblaze && npm run babel && npm run build && cd ~/geotiff.io && npm run less",
